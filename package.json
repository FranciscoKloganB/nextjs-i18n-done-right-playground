{
  "author": "WTX - cloudgeeks GmbH",
  "description": "Next.js - i18n done right",
  "name": "demo-i18n-with-rtl",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rimraf .next && next build",
    "dev": "next",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "eslint": "eslint --fix --ext \"**/*.{js,jsx,ts,tsx}\"",
    "export": "next export",
    "format": "prettier --write \"**/*.{css,json,js,jsx,ts,tsx}\"",
    "prepare": "husky install && chmod ug+x .husky/*",
    "start": "next start",
    "lint:next": "next lint",
    "lint:ts": "tsc --project tsconfig.json",
    "lint": "next lint && tsc --project tsconfig.json",
    "netlify": "npm ci && npm rebuild && npm run prepare --if-present && npm run build"
  },
  "dependencies": {
    "@emotion/css": "^11.5.0",
    "@emotion/react": "^11.5.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "next": "^12.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sharp": "^0.30.7",
    "swr": "^1.3.0",
    "usehooks-ts": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@emotion/babel-plugin": "^11.3.0",
    "@faker-js/faker": "^7.3.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^18.0.6",
    "babel-loader": "^8.2.3",
    "babel-plugin-macros": "^3.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "twin.macro": "^3.0.0-rc.2",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=16.5.0",
    "npm": ">=8.5.0",
    "yarn": "please-use-npm"
  }
}
