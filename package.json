    "react-dom": "^17.0.2",
    "react-i18next": "^11.18.1",
    "sharp": "^0.30.7",
    "swr": "^1.3.0",
    "tailwind-styled-components": "^2.1.8",
    "usehooks-ts": "^2.6.0"
